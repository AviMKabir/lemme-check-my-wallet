      <form name="form" class="form " action="POST">
        <div class="form-group">
          <input required="required" name="user_name" type="text" id="example-text" aria-describedby="example-text"
            placeholder="name">
        </div>
        <div class="form-group">
          <input required="required" name="user_budget" type="number" onkeydown="return event.keyCode !== 69"
            id="example-description" aria-describedby="example-description" placeholder="budget"></input>
        </div>
        <button id="submit" class="">Submit</button>
      </form>


    </div>

    <ul class="bg-bubbles">
